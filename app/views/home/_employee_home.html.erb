<div class="row">
  <%= render :partial => '/employees/employee_details', :locals => {:employee => current_user.employee }%>
</div>

<div class="row">
  <div class="span12">
    <% weekly_calendar(@events, :date => @date, :include_24_hours => true) do |w|%>
      <% w.week do |shift| %>
      <%= shift.starts_at.strftime('%I:%M%p')%>
      <%= link_to truncate(shift.store.name), shift_path(shift) %>
      <% end %>
    <% end %>
  <% weekly_links(:date => @date).html_safe %>
  </div>
</div>

