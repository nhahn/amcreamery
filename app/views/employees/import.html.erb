<% form_tag :url=>{:controller=>"employees", :action=>"parseCSV"}, :html => { :multipart => true } do %>
 <table>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= file_field :file, :csv %>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' %>
     </td>
   </tr>
 </table>
<% end %>
