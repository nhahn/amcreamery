<h1>Listing stores</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.name %></td>
    <td><%= store.street %></td>
    <td><%= store.city %></td>
    <td><%= store.state %></td>
    <td><%= store.zip %></td>
    <td><%= store.phone %></td>
    <td><%= store.latitude %></td>
    <td><%= store.longitude %></td>
    <td><%= store.active %></td>
    <td><%= link_to 'Show', store %></td>
    <td><%= link_to 'Edit', edit_store_path(store) %></td>
    <td><%= link_to 'Destroy', store, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_store_path %>
